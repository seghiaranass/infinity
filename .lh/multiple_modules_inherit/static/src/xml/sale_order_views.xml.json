ament number but ignore line sections -->\n        <xpath expr=\"//t[@t-call='{{ constructor.recordRowTemplate }}']\" position=\"after\">\n            <!-- <t t-if=\"!record.data.display_type or (record.data.display_type !== 'line_section' and record.data.display_type !== 'line_note')\"> -->\n                <t t-set=\"RowNumber\" t-value=\"RowNumber+1\" />\n            <!-- </t>             -->\n        </xpath>\n    </t>\n\n    <t t-name=\"row_column_number.sectionAndNoteListRenderer.RecordRowNumber\" t-inherit=\"web.ListRenderer.RecordRow\" t-inherit-mode=\"extension\" owl=\"1\">\n        <xpath expr=\"//td[@class='o_list_record_selector']\" position=\"before\">\n            <!-- Only show row number if it's not a section -->\n            <t t-if=\"!record.data.display_type or (record.data.display_type !== 'line_section' and record.data.display_type !== 'line_note')\">\n                <td tabindex=\"-1\">\n                    <span t-esc=\"RowNumber\" />\n                </td>\n            </t>\n        </xpath>\n    </t>\n</template>\n"
